extern puts: String => Int64

infix == 3

trait Eq a
  ==: a => a => Bool

impl Eq Int64
  ==: e, f -> ~IEq e f

impl Eq Bool
  ==: i, j -> ~BEq i j

use Eq::(*)

trait Print a
  print: a => Int64

impl Print Bool
  @print: ->
    if @
    then puts "true"
    else puts "false"

mdr: a, b -> a == b
mdr2: a, b -> a == b
lol: a, b -> mdr a, b == mdr2 2, 2

main: -> lol(2, 2).print()
