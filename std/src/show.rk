extern malloc Int64 -> String
extern sprintf String -> String -> Int64 -> Int64
extern fcvt Float64 -> Int64 -> String -> String -> String

trait Show a
    show a -> String

itoa a =
    let s = malloc 10
    sprintf s, "%d", a
    s

ftoa c =
    fcvt c, 10, (malloc 4), (malloc 4)

impl Show Int64
    show a = itoa a

impl Show Float64
    show c = ftoa c
