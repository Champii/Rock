use super::externs::puts
use super::externs::printf
use super::externs::fflush
use super::show::(*)
use super::eq::(*)
use super::num::(*)

trait Print
  @print: ->
    puts @show!
    # There is a bug that disallow self-returning function definition in traits (#174)
    @
  @putstr: ->
    printf "%s", @show!
    fflush 0
    # There is a bug that disallow self-returning function definition in traits (#174)
    @

# Here we would need generic impl
impl Print Bool
impl Print Int64
impl Print Float64
impl Print String
impl Print Char

impl Print [Int64]
