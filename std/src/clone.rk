use super::externs::malloc
use super::externs::strcpy
use super::externs::strlen

trait Clone a
  clone: a => a

impl Clone Int64
  @clone: -> @

impl Clone Float64
  @clone: -> @

impl Clone String
  @clone: ->
    let s = malloc strlen @
    strcpy s, @
    s

use super::functor::(*)

impl Clone [Int64]
  @clone: -> map clone, @

