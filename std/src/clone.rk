use super::externs::c_malloc
use super::externs::c_strcpy
use super::externs::c_strlen

trait Clone a
  clone :: a -> a

impl Clone Int64
  @clone = @

impl Clone Float64
  @clone = @

impl Clone String
  @clone =
    let s = c_malloc c_strlen @
    c_strcpy s, @
    s

use super::functor::(*)

impl Clone [Int64]
  @clone = map clone, @

