use super::externs::c_malloc
use super::externs::c_strcpy
use super::externs::c_strlen

trait Clone a
  clone :: a -> a

impl Clone Int64
  clone a = a

impl Clone Float64
  clone a = a

impl Clone String
  clone a =
    let s = c_malloc c_strlen a
    c_strcpy s, a
    s

use super::functor::(*)

impl Clone [Int64]
  clone a = map clone, a
