use super::num::(*)
use super::eq::(*)

map: f, arr ->
  let i = 0
  let arr2 = arr
  while i < (~Len arr arr)
    arr2[i] = f arr[i]
    i = i + 1
  arr2

foreach: f arr ->
  let i = 0
  while i < (~Len arr arr)
    f arr[i]
    i = i + 1
  arr

_shut_up_warnings: ->
  map 0, 0
  foreach 0, 0
  _shut_up_warnings!

